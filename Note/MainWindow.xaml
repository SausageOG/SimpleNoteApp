<Window x:Class="Note.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Note"
        mc:Ignorable="d"
        Title="Note App" Height="326.034" Width="323.11" ResizeMode="NoResize">
    <DockPanel LastChildFill="False">
        <StackPanel DockPanel.Dock="Top">
            <TextBox Width="200" Height="22" VerticalContentAlignment="Center" Margin="5"/>
            <Button Content="Add Note" FontSize="20" Width="250" Height="30" Margin="5"/>
        </StackPanel>
        <StackPanel DockPanel.Dock="Top">
            <ListBox Width="250" Height="200" Margin="10"/>
        </StackPanel>
    </DockPanel>
</Window>
